    <div class="position-relative">
      <section class="section section-lg section-hero section-shaped">
        <%= image_tag( "afg/sunset-cloth.jpg", class: "img-fluid shape shape-style-1" ) %>
        <div class="container shape-container d-flex align-items-center py-lg">
          <div class="col px-0 py-5">
            <div class="row align-items-center justify-content-center">
              <div class="col-lg-12 text-center">
                <div class="coming-soon">
                 <h1 class="display-1">MEETINGS</h1>
                  <h1 class="display-1">COMING SOON</h1>
                  <hr>
                  <h2 id="countdown">How long?</p>
                </div>
                <div class="mt-5">
                  <br>
                  <br>
                </div>
              </div>
            </div>
            <div class="mt-5"></div>
          </div>
        </div>
        <!-- SVG separator -->
        <div class="separator separator-bottom separator-skew zindex-100">
          <svg x="0" y="0" viewBox="0 0 2560 100" preserveAspectRatio="none" version="1.1" xmlns="http://www.w3.org/2000/svg">
            <polygon class="fill-white" points="2560 0 2560 100 0 100"></polygon>
          </svg>
        </div>
      </section>
    </div>
     
    <section class="section section-lg pt-lg-0 mt--200">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-lg-12">
            <div class="row row-grid">
              <div class="col-lg-12 px-0">
                <div class="card shadow border-0">
                  <div class="card-body py-5">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th scope="col">Name</th>
                          <th scope="col">Day</th>
                          <th scope="col">Time</th>
                          <th scope="col">Location</th>
                          <th scope="col">Street</th>
                          <th scope="col">City</th>
                          <!--th scope="col">Type</th-->
                          <th scope="col">Notes</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% @meetings.each do |meeting| %>
                          <tr>
                            <td>
                              <%# link_to meeting.name, meeting_path(meeting) %>
                              <%= meeting.name %>
                            </td>
                            <td><%= meeting.day.titleize %></td>
                            <td><%= meeting.time.strftime("%l:%M %p") %></td>
                            <td><%= meeting.location %></td>
                            <td><%= meeting.street %></td>
                            <td><%= meeting.city %></td>
                            <!--td><%# meeting.category.titleize %></td-->
                            <td><%= meeting.notes if meeting.notes %>
                          </tr>
                        <% end %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
